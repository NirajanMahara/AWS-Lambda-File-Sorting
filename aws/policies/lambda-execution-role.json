        {
            "Effect": "Allow",
            "Action": [
                "s3:GetObject",
                "s3:PutObject",
                "s3:ListBucket"
            ],
            "Resource": [
                "arn:aws:s3:::sortin-nirajan-0921977",
                "arn:aws:s3:::sortin-nirajan-0921977/*",
                "arn:aws:s3:::sortout-nirajan-0921977",
                "arn:aws:s3:::sortout-nirajan-0921977/*"
            ]
        }
    ]
} 